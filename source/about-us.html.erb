  <h3><%= role.name %><span class="visuallyhidden"> &ndash; </span><span class="about-us__qualification"><%= role.qualification %></span></h3>
  <% if role.extras %>
    <p class="about-us__extras">Other qualifications &ndash; <%= role.extras %></p>
  <% end %>
  <% if role.gdc %>
    <p><span class="sc">GDC</span> number &ndash; <span class="lnum"><%= role.gdc %></span></p>
